#!/bin/bash
echo "$0 $*" >> "$HOME"/python.log
if [[ -x env/bin/python ]]; then
  exec env/bin/python "$@"
else
  if command -v python3 >/dev/null 2>/dev/null; then
    exec python3 "$@"
  fi

  echo "pseudo-python: could not find env/bin/python. you probably need to create your venv" >&2
  exit 1
fi
